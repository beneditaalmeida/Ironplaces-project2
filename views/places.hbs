<h1>qualquercoisa</h1>
{{#each places}}
<div class="card d-flex justify-content-around align-self-stretch" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <h6 class="card-subtitle mb-2 text-muted">{{this.address}}</h6>
    <p class="card-text">{{this.city}}</p>
    <p class="card-text">{{this.zip}}</p>
    <p class="card-text">{{this.description}}</p>
    <p class="card-text">{{this.category}}</p>
  </div>
  
