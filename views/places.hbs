<h1>qualquercoisa</h1>
{{#each places}}
<div class="card d-flex justify-content-around align-self-stretch" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <h6 class="card-subtitle mb-2 text-muted">{{this.address}}</h6>
    <p class="card-text">{{this.city}}</p>
    <p class="card-text">{{this.zip}}</p>
    <p class="card-text">{{this.description}}</p>
    <p class="card-text">{{this.category}}</p>
    <a class="btn btn-primary" href={{this.link}}>Open in GoogleMaps</a>
    <a class="clbtn btn-outline-info" src={{this.link}}></a>
    <div class="embed-responsive embed-responsive-16by9">
  <iframe class="embed-responsive-item" src={{this.link}} allowfullscreen></iframe>
</div>
  </div>
{{#ifEquals role "ADMIN"}}
<form action="/edit-place/{{this._id}}" method="get">
<button class="btn btn-danger">Edit</button>
</form>
<div>
<form action="/delete-place/{{this._id}}" method="get">
<button class="btn btn-danger">Delete</button>
</form>
{{/ifEquals}}
</div>
</div>
{{/each}}


